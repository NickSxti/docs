{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "Qonversion",
  "colors": {
    "primary": "#a07fff",
    "light": "#a07fff",
    "dark": "#7c4dff"
  },
  "favicon": "/59c36df-small-Avatars_500x500_Black.png",
  "search": {
    "prompt": "Search Qonversion docs..."
  },
  "appearance": {
    "default": "system"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "seo": {
    "indexing": "all"
  },
  "repository": {
    "url": "https://github.com/nicksxti/docs",
    "branch": "main"
  },
  "footerSocials": {
    "github": "https://github.com/qonversion",
    "x": "https://x.com/qonabornikov",
    "linkedin": "https://www.linkedin.com/company/qonversion"
  },
  "api": {
    "playground": {
      "mode": "simple"
    },
    "examples": {
      "languages": [
        "curl",
        "python",
        "javascript",
        "php",
        "ruby",
        "go",
        "java"
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Home",
        "href": "https://qonversion.io"
      },
      {
        "label": "Dashboard",
        "href": "https://dash.qonversion.io"
      },
      {
        "label": "Status",
        "href": "https://status.qonversion.io"
      }
    ]
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/guides/qonversion-platform"
    },
    {
      "source": "/docs/:path*",
      "destination": "/guides/:path*"
    }
  ],
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "guides/qonversion-platform",
              "guides/how-qonversion-works"
            ]
          },
          {
            "group": "Testing",
            "pages": [
              "guides/testing",
              "guides/troubleshooting"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "href": "https://qonversion.io"
      },
      {
        "tab": "Changelog",
        "pages": [
          "changelog"
        ]
      }
    ]
  }
}
